digraph {
	layout = dot;
	
	node [shape = point];
	nodesep = .125;
	#rankdir = LR;
	dpi = 72;
	
	A0 -> B0;
	A1 -> B1;
	A2 -> B2;
	A3 -> B3 [style = dotted, dir = none];
	A3, B3 [style = invis];
	A4 -> B4 [style = dotted, dir = none];
	A4, B4 [style = invis];
	A5 -> B5 [style = dotted, dir = none];
	A5, B5 [style = invis];
}